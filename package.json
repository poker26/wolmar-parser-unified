{
  "name": "meshok-parser",
  "version": "0.1.0",
  "description": "Parser for Meshok.net auction data - coins price history analysis",
  "main": "index.js",
  "scripts": {
    "fetch:listing": "node scripts/fetch-listing.js",
    "fetch:listing:advanced": "node scripts/fetch-listing-advanced.js",
    "fetch:chrome": "node scripts/fetch-with-chrome.js",
    "fetch:system-chrome": "node scripts/fetch-with-system-chrome.js",
    "fetch:stealth": "node scripts/fetch-stealth.js",
    "fetch:multiple": "node scripts/fetch-multiple-attempts.js",
    "fetch:ultimate": "node scripts/fetch-ultimate.js",
    "fetch:playwright": "node scripts/fetch-playwright.js",
    "fetch:xvfb": "node scripts/fetch-xvfb.js",
    "fetch:undetected": "node scripts/fetch-undetected.js",
    "fetch:automated": "node scripts/fetch-fully-automated.js",
    "fetch:proxy": "node scripts/fetch-with-proxy.js",
    "fetch:alternative": "node scripts/fetch-alternative.js",
    "fetch:stealth:advanced": "node scripts/fetch-stealth-advanced.js",
    "fetch:simple": "node scripts/fetch-simple-bypass.js",
    "fetch:python": "python3 scripts/fetch-python-selenium.py",
    "fetch:undetected": "python3 scripts/fetch-undetected-chrome.py",
    "fetch:undetected:fixed": "python3 scripts/fetch-undetected-fixed.py",
    "fetch:cloudscraper": "python3 scripts/fetch-cloudscraper.py",
    "fetch:httpx": "python3 scripts/fetch-httpx.py",
    "fetch:simple:requests": "python3 scripts/fetch-simple-requests.py",
    "fetch:simple:python": "python3 scripts/fetch-simple-python.py",
    "fetch:session": "python3 scripts/fetch-with-session.py",
    "fetch:curl:session": "chmod +x scripts/fetch-curl-session.sh && ./scripts/fetch-curl-session.sh",
    "fetch:curl:advanced": "chmod +x scripts/fetch-curl-advanced.sh && ./scripts/fetch-curl-advanced.sh",
    "fetch:browser:extension": "node scripts/fetch-browser-extension.js",
    "fetch:browser:system": "node scripts/fetch-browser-system.js",
    "fetch:browser:headless": "node scripts/fetch-browser-headless.js",
    "install:chrome": "chmod +x scripts/install-chrome.sh && ./scripts/install-chrome.sh",
    "install:xvfb": "chmod +x scripts/install-xvfb.sh && ./scripts/install-xvfb.sh",
    "setup:linux": "chmod +x scripts/setup-linux-environment.sh && ./scripts/setup-linux-environment.sh",
    "start:proxy": "chmod +x scripts/start-browser-proxy.sh && ./scripts/start-browser-proxy.sh",
    "test:proxy": "node scripts/test-browser-proxy.js",
    "check:chrome": "python3 scripts/check-chrome.py",
    "fetch:xvfb:playwright": "xvfb-run --auto-servernum node scripts/fetch-xvfb-playwright.js",
    "analyze:files": "node scripts/analyze-saved-files.js",
    "find:api": "node scripts/find-hidden-api.js",
    "quick:analyze": "node scripts/quick-analyze.js",
    "fetch:curl": "node scripts/fetch-curl.js",
    "fetch:curl:advanced": "node scripts/fetch-curl-advanced.js",
    "fetch:item": "node scripts/fetch-item.js",
    "analyze": "node scripts/analyze-structure.js",
    "test": "node scripts/test-fetch.js",
    "install:deps": "chmod +x scripts/install-dependencies.sh && ./scripts/install-dependencies.sh",
    "install:tools": "chmod +x scripts/install-tools.sh && ./scripts/install-tools.sh"
  },
  "keywords": ["meshok", "parser", "coins", "numismatics", "auction"],
  "author": "Oleg",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^21.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "playwright": "^1.40.0",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  }
}